{
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "podinstall": "cd ios && pod install --repo-update && cd -",
    "ios": "yarn run podinstall && react-native run-ios",
    "web": "expo start --web --localhost",
    "xcode": "open ./ios/Mobileflashcardapp.xcworkspace || open ./ios/Mobileflashcardapp.xcodeproj",
    "start:native": "react-native start --reset-cache",
    "start": "react-native start",
    "clean:xcode": "rm -rf ~/Library/Developer/Xcode/DerivedData/ && cd ios && rm -rf Pods/ &&  xcodebuild clean && cd ..",
    "clean:metro": "watchman watch-del-all && rm -rf /tmp/metro-*",
    "clean:yarn": " yarn cache clean",
    "clean": "yarn run clean:metro && yarn run clean:xcode && rm -rf node_modules/",
    "clean-start": "yarn run clean:yarn && yarn run clean && yarn install && yarn start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "5.11.8",
    "@react-navigation/drawer": "^5.3.0",
    "@react-navigation/native": "^5.0.8",
    "@react-navigation/stack": "^5.1.0",
    "@reduxjs/toolkit": "^1.5.0",
    "babel-preset-expo": "^8.3.0",
    "bufferutil": "^4.0.1",
    "canvas": "^2.5.0",
    "jsdom": "16.4.0",
    "react": "16.13.1",
    "react-dom": "~16.9.0",
    "react-native": "~0.63.4",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-pod": "^1.12.1",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.2",
    "react-native-splash-screen": "^3.2.0",
    "react-native-unimodules": "~0.12.0",
    "react-native-web": "~0.13.7",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "utf-8-validate": "^5.0.2",
    "ws": "7.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "babel-jest": "~25.2.6",
    "jest": "~25.2.6",
    "metro-react-native-babel-preset": "~0.56.0",
    "metro-react-native-babel-transformer": "^0.65.1",
    "react-test-renderer": "~16.13.1",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true,
  "name": "Mobile-flash-card-app",
  "version": "1.0.0"
}
